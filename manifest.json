{
	"manifest_version": 2,
	"name": "Досаден локал.се сабскрајбер",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://www.thelocal.se/*"],
			"js": ["thelocal.js"]
		}
	],
	"browser_action": {
		"default_icon": "icon.png"
	}
}
